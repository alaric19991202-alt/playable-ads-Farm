{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "lib": ["ES2020", "DOM"],
    "strict": false,
    "skipLibCheck": true,
    "noEmit": true,
    "useDefineForClassFields": true,
    "isolatedModules": true,
    "resolveJsonModule": true
  },
  "include": ["src"]
}
